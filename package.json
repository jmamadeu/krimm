{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "description": "React, NextJS, Boilerplate",
  "author": "jmamadeu",
  "keywords": [
    "nextjs",
    "contentful"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "format": "prettier --write .",
